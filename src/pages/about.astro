---
import Layout from "../layouts/Layout.astro";
const allPost = await Astro.glob("./posts/*.md");
const content = "abc";
---

<Layout title="About">
    <h1>About</h1>
    <p>{content}</p>
    <ul>
        {
            allPost.map((post) => (
                <li>
                    <a href={post.url}>{post.frontmatter.title}</a>
                </li>
            ))
        }
    </ul>
</Layout>
